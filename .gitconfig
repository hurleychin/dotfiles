[push]
	default = simple
[user]
	name = hurleychin
	email = qinhulin@gmail.com
[core]
	autocrlf = input
	quotepath = false
	filemode = false
[diff]
    tool = kompare
[difftool]
	prompt = false
[pull]
	rebase = true
[safe]
	directory = /home/qinhulin/hgfs/git/java-practice
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
[init]
	defaultBranch = master
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
